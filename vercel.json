{
  "version": 2,
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}
